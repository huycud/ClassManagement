@model ClassManagement.Mvc.Models.Manager.UpdateAdminViewModel

@{
    Layout = "";
}

<div class="row">
    <div class="col-md-12">
        <form asp-action="Update" onsubmit="return jQueryPost('#profile',this,'Profile');">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">#</div>
                    </div>
                    <input asp-for="Email" class="form-control" type="text" readonly/>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="UserName" class="control-label"></label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">#</div>
                    </div>
                    <input asp-for="UserName" class="form-control" type="text" readonly />
                </div>
            </div>
            <div class="form-group">
                <label asp-for="Fullname" class="control-label"></label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-text">#</div>
                    </div>
                    <input asp-for="Fullname" class="form-control" />
                </div>
                <span asp-validation-for="Fullname" class="text-danger"></span>
            </div>
            <div class="form-group col-md-12 mt-3">
                <input type="submit" value="Lưu" class="btn btn-danger ip-custom btn-custom" />
            </div>
        </form>
    </div>
</div>
